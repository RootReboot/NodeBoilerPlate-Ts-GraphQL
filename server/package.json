{
  "name": "server",
  "version": "0.0.1",
  "description": "Awesome project developed with TypeORM.",
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/jest": "^25.1.1",
    "@types/node": "^13.5.0",
    "@types/uuid": "^3.4.7",
    "aws-lambda": "0.1.2",
    "gql2ts": "^1.10.1",
    "graphql": "^14.6.0",
    "graphql-yoga": "^1.18.3",
    "jest": "^25.1.0",
    "nodemon": "^2.0.2",
    "ts-jest": "^25.2.0",
    "ts-node": "8.6.2",
    "tslint": "^6.0.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "3.7.5"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "graphql-import": "^1.0.0-beta.2",
    "graphql-tools": "^4.0.6",
    "pg": "^7.17.1",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.22",
    "uuid": "^3.4.0"
  },
  "scripts": {
    "start": "SET NODE_ENV=development&nodemon --exec ts-node --files src/index.ts",
    "test": "SET NODE_ENV=test&jest",
    "gen-schema-types": "gql2ts src/schema.graphql -o src/types/schema.d.ts"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
